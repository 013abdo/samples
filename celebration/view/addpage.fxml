            <Label fx:id="check" prefHeight="51.0" prefWidth="310.0" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Label text="confirm" GridPane.rowIndex="2" />
            <PasswordField fx:id="confirm" promptText="Enter your pass" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Button mnemonicParsing="false" onAction="#Cancel" text="Cancle" GridPane.rowIndex="4" />
         </children>
      </GridPane>
      <Label layoutX="184.0" layoutY="14.0" prefHeight="53.0" prefWidth="138.0" text="Registor">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Button layoutX="232.0" layoutY="361.0" mnemonicParsing="false" onAction="#login" text="login" />
   </children>
</AnchorPane>
